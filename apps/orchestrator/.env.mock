# Runtime
NODE_ENV=development
PORT=8787

# Provider mode: mock | sandbox | prod
PROVIDER_MODE=mock

# Optional semantic model override
SEMANTIC_MODEL_PATH=

# Planning and synthesis controls
REAL_FAST_PLAN_STEPS=2
REAL_DEEP_PLAN_STEPS=4
REAL_ENABLE_PARALLEL_SQL=false
REAL_MAX_PARALLEL_QUERIES=3
REAL_LLM_TEMPERATURE=0.1
REAL_LLM_MAX_TOKENS=1400

# Stream pacing (mock mode only)
MOCK_STREAM_STATUS_DELAY_MS=700
MOCK_STREAM_TOKEN_DELAY_MS=120
MOCK_STREAM_RESPONSE_DELAY_MS=450
